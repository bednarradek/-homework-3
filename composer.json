    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "require": {
        "php": ">=8.0",
        "nette/database": "^3.1",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "nette/di": "^3.0",
        "nette/neon": "^3.3",
        "nette/caching": "^3.1",
        "robmorgan/phinx": "^0.12.9",
        "fzaninotto/faker": "^1.5",
        "nette/finder": "^2.5",
        "nette/schema": "^1.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "stan": "vendor/bin/phpstan analyse bin src index.php --level 7",
        "phpcs": "phpcs --standard=PSR12 bin src index.php",
        "phpcbf": "phpcbf --standard=PSR12 bin src index.php"
    }
